            $('#plans-hide-content').animate({height: $("#plans-hide-content").get(0).scrollHeight}, 500, 'swing', function() {
                $this.removeClass('closed')
            }).removeClass('closed')
            setCookie('profile-cut-closed', 0, 1)
        } else {
            $this.addClass('closed')
            $('html, body').stop().animate({scrollTop: document.documentElement.scrollTop - 500}, 500, 'swing')
            $('#plans-hide-content').animate({height: '310px'}, 500, 'swing', function() {
                $('#plans-hide-content').addClass('closed')
                $this.addClass('closed')
            })
            setCookie('profile-cut-closed', 1, 31*24*60*60*1000)
        }
    })
    
